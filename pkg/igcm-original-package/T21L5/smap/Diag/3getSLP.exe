#!/usr/bin/sh

clean
#script to get Z500
#convert history file to single precision
#
BEGDAY=1.
ENDDAY=1.
ISTEP=1
ITSTEPS=48

echo 'compiling getSLPpre.f'
f77 -o getSLPpre getSLPpre.f

INFILE=Enssum.dat

echo 'running getSLPpre'

cat << /EOF | getSLPpre $INFILE
$BEGDAY
$ENDDAY
$ISTEP
/EOF


#prepare input to presmap
#
cat << /EOF > temphead
COMMON
  TEMPDIR=/zemo2/jiaxj/igcm/EXPERIMENTS/iAMIP_1
  HOMEDIR=/zemo2/jiaxj/igcm/EXPERIMENTS/iAMIP_1
  CURRENTDIR=/zemo2/jiaxj/igcm/EXPERIMENTS/iAMIP_1
  BEGDAY=$BEGDAY
  ENDDAY=$ENDDAY
  DIAGNOSTICS=SG
  UTF=1.4
  LEVELTYPE=IS
  PRINT=T
  PLOT=F
  PRINTSIGFIG=3
  ASCIICHARS=2
  SCRATCH=F
  OROGRAPHY=F
  FREQSAMPLING=$ITSTEPS
  FREQPRINT=$ITSTEPS
/EOF
cat temphead presmap11.SLPSG > fort.10
#
#run presmap and smap
#
echo running presmap
./presmap11.out
ln -s data.res.sp fort.9

echo running smap
./smap11.out
#mv gridout /pike/jiaxj/igcm/Result/Conren/SLPdaily.dat 
mv gridout /zemo2/jiaxj/Internal/Prediction/Test/ForeExp/SLPdaily.dat 
#mv gridout /zemo2/jiaxj/Internal/Prediction/NewdampData/SLPdaily.dat 
rm -f fort.*
rm -f data.*
rm tem*
rm nre*
